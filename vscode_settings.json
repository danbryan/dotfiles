    // "go.inferGopath": false,
    "git.defaultCloneDirectory": "~/code",
    "gitProjectManager.baseProjectsFolders": [
        "~/code",
        "~/go/src"
    ],
    "editor.minimap.enabled": false,
    "git.enableSmartCommit": true,
    "git.confirmSync": false,
    "workbench.iconTheme": "material-icon-theme",
    "files.associations": {
        "*.template": "yaml",
        "*.cft": "yaml",
        "*.yml": "yaml",
        "*.env": "yaml"
    },
    "explorer.confirmDelete": false,
    "powermode.enabled": true,
    "terminal.integrated.fontSize": 12,
    "git.autofetch": true,
    "explorer.confirmDragAndDrop": false,
    "python.jediEnabled": true,
    "python.formatting.provider": "autopep8",
    "javascript.updateImportsOnFileMove.enabled": "always",
    "terminal.external.linuxExec": "konsole",
    "terraform.indexing": {
        "enabled": false,
        "liveIndexing": false,
        "delay": 50,
        "exclude": [
            ".terraform/**/*",
            "**/.terraform/**/*"
        ]
    },
    "terraform.languageServer": {
        "enabled": true,
        "args": []
    },
    "powermode.presets": "flames",
    "terminal.integrated.fontFamily": "MesloLGL Nerd Font Mono",
    "terminal.integrated.shell.linux": "/usr/bin/zsh",
    "go.formatTool": "goimports",
    "go.useLanguageServer": true,
    "git.enableCommitSigning": true,
    "git.fetchOnPull": true,
    "gitlens.views.compare.files.layout": "tree",
    "workbench.colorTheme": "Abyss",
    "materialTheme.accent": "Bright Teal",
    "yaml.customTags": [
        "!And",
        "!If",
        "!Not",
        "!Equals",
        "!Or",
        "!FindInMap",
        "!Base64",
        "!Cidr",
        "!Ref",
        "!Sub",
        "!GetAtt",
        "!GetAZs",
        "!ImportValue",
        "!Select",
        "!Split",
        "!Join",
        "!And sequence",
        "!If sequence",
        "!Not sequence",
        "!Equals sequence",
        "!Or sequence",
        "!FindInMap sequence",
        "!Join sequence",
        "!Sub sequence",
        "!ImportValue sequence",
        "!Select sequence",
        "!Split sequence"
    ],
    "yaml.format.enable": true,
    "python.linting.flake8Args": [
        "--max-line-length=240",
        "--ignore=D100"
    ],
    "vs-kubernetes": {
        "vs-kubernetes.helm-path": "/home/dan/.vs-kubernetes/tools/helm/linux-amd64/helm",
        "vs-kubernetes.draft-path": "/home/dan/.vs-kubernetes/tools/draft/linux-amd64/draft",
        "vs-kubernetes.minikube-path": "/home/dan/.vs-kubernetes/tools/minikube/linux-amd64/minikube"
    },
    "yaml.format.printWidth": 200,
    "python.linting.pylintEnabled": false,
    "errorLens.useColorContributions": true,
    "errorLens.annotationPrefix": [
        "❌: ",
        "⚠: ",
        "ℹ: "
    ],
    "errorLens.gutterIconsEnabled": true,
    "errorLens.gutterIconSet": "default",
    "errorLens.followCursor": "closestProblem",
    "errorLens.addAnnotationTextPrefixes": true,
    "workbench.statusBar.visible": true,
    "workbench.activityBar.visible": true,
    "go.autocompleteUnimportedPackages": true,
    "python.linting.flake8Path": "~/.virtualenvs/tools/bin/flake8",
    "python.linting.flake8Enabled": true,
    "cSpell.userWords": [
